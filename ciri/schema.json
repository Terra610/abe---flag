{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "CIRI inputs.csv header schema",
  "type": "object",
  "properties": {
    "cases_avoided": {"type":"number","minimum":0},
    "avg_cost_per_case": {"type":"number","minimum":0},
    "jail_days_avoided": {"type":"number","minimum":0},
    "cost_per_jail_day": {"type":"number","minimum":0},
    "fees_canceled_total": {"type":"number","minimum":0},
    "employment_probability": {"type":"number","minimum":0},
    "avg_monthly_wage": {"type":"number","minimum":0},
    "months_effective": {"type":"number","minimum":0},
    "expected_lawsuits": {"type":"number","minimum":0},
    "avg_payout": {"type":"number","minimum":0},
    "multiplier": {"type":"number","minimum":0},
    "transition_costs_one_time": {"type":"number","minimum":0}
  },
  "required": [
    "cases_avoided","avg_cost_per_case","jail_days_avoided","cost_per_jail_day",
    "fees_canceled_total","employment_probability","avg_monthly_wage","months_effective",
    "expected_lawsuits","avg_payout","transition_costs_one_time"
  ],
  "additionalProperties": true
}
